{
    "version": 2,
  "builds": [{
      "src": "nyelix/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
  },{
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": { "output": "static/src/styles.css" }
  }],
  "routes": [
    {
        "src": "/static/(.*)",
        "dest": "static/src/$1"
    },  
    {
          "src": "/(.*)",
          "dest": "nyelix/wsgi.py"
      }
  ]
}